---
import Layout from "~/layouts/PageLayoutse.astro";

import { SITE } from "~/config.mjs";

import Hero from "~/components/widgets/Hero.astro";
import Herologose from "~/components/widgets/Herologose.astro";
import BasicFeaturesse from "~/components/widgets/BasicFeaturesse.astro";

import StepsLeftse from "~/components/widgets/StepsLeftse.astro";
import StepsLeft2se from "~/components/widgets/StepsLeft2se.astro";
import StepsLeft3se from "~/components/widgets/StepsLeft3se.astro";
import StepsRightse from "~/components/widgets/StepsRightse.astro";
import StepsRight2se from "~/components/widgets/StepsRight2se.astro";
import StepsRight3se from "~/components/widgets/StepsRight3se.astro";


const title = `Skapa anpassade fula jultröjor — ${SITE.name}`;
const description = `Det här är det perfekta plagget för ditt team - perfekt för återförsäljning, kampanjer, marknadsföring, presenter och allt annat skoj.`;
const canonical = new URL("", Astro.site);
---

<Layout meta={{ title, description, canonical }}>
  <Hero />
  <Herologose />
  <BasicFeaturesse />
  <StepsLeftse />
  <StepsRightse />
  <StepsRight2se />
  <StepsLeft2se />
  <StepsRight3se />
  <StepsLeft3se />
