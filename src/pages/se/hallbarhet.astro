---
import Layout from "~/layouts/PageLayoutse.astro";

import { SITE } from "~/config.mjs";


import BasicEthosse from "~/components/widgets/BasicEthosse.astro";
import Ethosse from "~/components/widgets/Ethosse.astro";


import StepsEthosse from "~/components/widgets/StepsEthosse.astro";


const title = SITE.title;
const description = SITE.description;
const canonical = new URL("", Astro.site);
---

<Layout meta={{ title, description, canonical }}>
  <StepsEthosse />
  <BasicEthosse />
  <Ethosse />

</Layout>