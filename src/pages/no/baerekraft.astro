---
import Layout from "~/layouts/PageLayoutno.astro";

import { SITE } from "~/config.mjs";


import BasicEthosno from "~/components/widgets/BasicEthosno.astro";
import Ethosno from "~/components/widgets/Ethosno.astro";


import StepsEthosno from "~/components/widgets/StepsEthosno.astro";


const title = SITE.title;
const description = SITE.description;
const canonical = new URL("", Astro.site);
---

<Layout meta={{ title, description, canonical }}>
  <StepsEthosno />
  <BasicEthosno />
  <Ethosno />

</Layout>